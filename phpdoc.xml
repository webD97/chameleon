<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
  <parser>
    <target>temp/phpdoc</target>
    <encoding>utf8</encoding>
  </parser>
  <transformer>
    <target>docs/documentation</target>
  </transformer>
  <files>
    <directory>src</directory>
  </files>
  <transformations>
    <template name="xml" />
  </transformations>
</phpdoc>