{
    "name": "webd97/chameleon",
    "description": "PHP image manipulation library built around GD",
    "license": "MIT",
    "keywords": ["image", "manipulation", "creation", "gd", "gd2", "png"],
    "require": {
        "php": "^7.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/webd97/phpdoc-md"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "codeclimate/php-test-reporter": "dev-master",
        "phpdocumentor/phpdocumentor": "^2.9",
        "evert/phpdoc-md": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Chameleon\\": "src/"
        }
    }
}
